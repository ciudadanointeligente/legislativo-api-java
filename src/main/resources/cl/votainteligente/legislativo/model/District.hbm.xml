<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cl.votainteligente.legislativo.model">
	<class name="District">
		<id name="id">
			<generator class="identity" />
		</id>
		<property name="name" />
		<many-to-one name="circunscription" column="id"/>
		<many-to-one name="region"/>
	</class>
</hibernate-mapping>
